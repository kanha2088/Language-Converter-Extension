{
    "manifest_version": 3,
    "name": "Word Popup Extension",
    "version": "1.0",
    "description": "Displays a popup with a word when double-clicked.",
    "permissions": ["activeTab","storage"],
   
    "action": {
      "default_popup": "pop.html",
      "default_icon": {
         "16":"nlogo.png",
         "48":"mlogo.png",
         "128": "llogo.png"
      }
    },
    "icons": {
         "16":"nlogo.png",
         "48":"mlogo.png",
         "128": "llogo.png"
    }
  }
  